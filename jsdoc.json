{
	"tags": {
		"allowUnknownTags": false
	},
	"source": {
		"include": "src",
		"includePattern": "\\.js$",
		"excludePattern": "(node_modules/|docs|__tests__)"
	},
	"opts": {
		"template": "node_modules/clean-jsdoc-theme",
		"encoding": "utf8",
		"destination": "docs/",
		"recurse": true,
		"verbose": true,
		"theme_opts": {
			"search": true,
			"homepageTitle": "@untemps/utils",
			"default_theme": "dark",
			"title": "@untemps/utils",
			"footer": "@untemps @ 2022",
			"meta": [
				{
					"name": "Author",
					"content": "Vincent Le Badezet"
				},
				{
					"name": "Description",
					"content": "Utils for @untemps packages"
				}
			],
			"menu": [
				{
					"title": "Github",
					"id": "github",
					"link": "https://github.com/untemps/utils"
				},
				{
					"title": "npm",
					"id": "npm",
					"link": "https://www.npmjs.com/package/@untemps/utils"
				}
			],
			"resizeable": {
				"navbar": {
					"min": "300",
					"max": "600"
				}
			},
			"codepen": {
				"enable_for": ["examples"],
				"options": {
					"js_pre_processor": "babel"
				}
			}
		}
	},
	"templates": {
		"cleverLinks": false,
		"monospaceLinks": false,
		"default": {
			"outputSourceFiles": true,
			"includeDate": false,
			"useLongnameInNav": true
		}
	},
	"markdown": {
		"hardwrap": false,
		"idInHeadings": true
	}
}
